{
  "expo": {
    "name": "One Minute Experience",
    "slug": "one-minute",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f6f0ea"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "dk.itu.oneminute",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to identify the artworks you see in the museum."
      }
    },
    "android": {
      "package": "dk.itu.oneminute"
    }
  }
}
