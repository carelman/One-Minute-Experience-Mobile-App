{
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "rules": {
    "semicolon": [true, "always"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "interface-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "import-name": false
  }
}
